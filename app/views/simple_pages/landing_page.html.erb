<div id="container-main">
  <div id="welcome">
    <h1>Welcome</h1>
      <h2> to Free's Essential Oil's</h2>
  </div> <!-- end welcome -->
<div class="container">
  <!-- <%= @products.name %> -->
<!-- INDICATORS -->
<div id="carousel-example-generic" class="carousel slide" data-ride="carousel">
  <!-- Indicators -->
  <ol class="carousel-indicators">
    <% @products.each_with_index do |product, index| %>
    <li data-target="#carousel-example-generic" data-slide-to="<%= index %>" class="<%= " active" if product == @products.first %>"></li>
    <% end %>
  </ol>

  <!-- WRAPPER FOR SLIDES-->
  <div class="carousel-inner" role="listbox">
    <% @products.each do |product| %>
      <div class="carousel-item<%= " active" if product == @products.first %>">
        <%= image_tag(product.image_url, class: "img-responsive") %>
        <div class="carousel-caption">
          <%= link_to product.name, product %>
        </div>
      </div>
      <% end %>
    </div>

